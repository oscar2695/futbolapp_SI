<zk>
	<window width="100%" border="normal"
		style="background-color: #014B94" apply="org.zkoss.bind.BindComposer"
		viewModel="@id('vm') @init('zktest.MatchesVM')">
		<hbox pack="center" width="100%">
			<vbox width="20%">
				<image
					src="data:image/jpeg;base64," />
			</vbox>
			<vbox width="750px">
				<image
					src="http://www.mundodeportivo.com/r/GODO/MD/p3/ContraPortada/Imagenes/2015/05/26/Recortada/img_hcoca_20150526-185605_imagenes_md_otras_fuentes_logo-liga-bbva-k3wE--952x225@MundoDeportivo-Web.png"
					width="100%" height="100%" />
			</vbox>
			
			<vbox width="20%">
				<image
					src="data:image/jpeg;base64," />
			</vbox>
		</hbox>
		<window title="Insertar Partido" border="normal" mode="modal"
			visible="@load(vm.currentMatch ne null)">
			<vbox>

				<hbox>
					<vbox width="110px">Equipo Local:</vbox>
					<vbox width="150px">
						<combobox id="equipolocalcombo"
							model="@load(vm.teams)"
							selectedItem="@load(vm.currentMatch.teamLocal) @save(vm.currentMatch.teamLocal, before='save')">
						</combobox>
						<zscript><![CDATA[
					    import org.zkoss.zul.*;
					    import persistencetest.*;
					    equipolocalcombo.setItemRenderer(new ComboitemRenderer(){ 
					    	void render(item,team,index) {
					    		item.setLabel(team.getName());
					    	}
					    	});					 
					]]>
					</zscript>
					</vbox>
				</hbox>
				
				<hbox>
				<vbox width="110px">
					Equipo Visitante:
				</vbox>
				<vbox width="150px">
					<combobox id="equipovisitantecombo" model="@load(vm.teams)"
					selectedItem="@load(vm.currentMatch.teamVisitor) @save(vm.currentMatch.teamVisitor, before='save')">
					</combobox>
					<zscript><![CDATA[
					    import org.zkoss.zul.*;
					    import persistencetest.*;
					    equipovisitantecombo.setItemRenderer(new ComboitemRenderer(){ 
					    	void render(item,team,index) {
					    		item.setLabel(team.getName());
					    	}
					    	});					 
					]]>
					</zscript>
				</vbox>
				</hbox>
				
				
				<hbox>
				<vbox width="110px">
					Goles Local:
				</vbox>
				<vbox width="150px">
					<intbox constraint="no negative" width="150px" 
					value="@load(vm.currentMatch.goalsLocal) @save(vm.currentMatch.goalsLocal, before='save')"/>
				</vbox>
				</hbox>
				
				<hbox>
				<vbox width="110px">
					Goles Visitante:
				</vbox>
				<vbox width="150px">
					<intbox constraint="no negative" width="150px"
					value="@load(vm.currentMatch.goalsVisitor) @save(vm.currentMatch.goalsVisitor, before='save')"/>
				</vbox>
				</hbox>
				
				
				<hbox>
				<vbox width="110px">
					Fecha:
				</vbox>
				<vbox width="150px">
						<datebox id="db" width="150px"
						value="@load(vm.currentMatch.dateMatch) @save(vm.currentMatch.dateMatch, before='save')" />
				</vbox>
				</hbox>
				
				
				<hbox>
				<vbox width="110px">
					Asistencia:
				</vbox>
				<vbox width="150px">
					<intbox constraint="no negative" width="150px"
					value="@load(vm.currentMatch.spectators) @save(vm.currentMatch.spectators, before='save')"/>
				</vbox>
				</hbox>
			
			</vbox>
			<button label="Guardar" onClick="@command('save')"></button>
			<button label="Cancelar" onClick="@command('cancelMatch')"></button>
		</window>
		<vbox>
			<button label="cerrar sesión" onClick="@command('logout')" />
		</vbox>

		<vbox>
			<separator height="10px" />
			<groupbox mold="3d" closable="false">
				<caption label="Insertar nuevo partido">
					<button label="+" onClick="@command('newMatch')" />
				</caption>
				<listbox model="@load(vm.matches)">
					<listhead>
						<listheader style="color:green;font-size:10pt" align="center" label="Equipo Local" />
						<listheader style="color:green;font-size:10pt" align="center" label="Equipo Visitante" />
						<listheader style="color:green;font-size:10pt" align="center" label="Goles Local" />
						<listheader style="color:green;font-size:10pt" align="center" label="Goles Visitante" />
						<listheader style="color:green;font-size:10pt" align="center" label="Fecha" />
						<listheader style="color:green;font-size:10pt" align="center" label="Estadio" />
						<listheader style="color:green;font-size:10pt" align="center" label="Capacidad" />
						<listheader style="color:green;font-size:10pt" align="center" label="Asistencia" />
						<listheader style="color:green;font-size:10pt" align="center" label="Eliminar" />
					</listhead>
					<template name="model">
						<listitem>
							<listcell>
								<label
									value="@load(each.teamLocal.name)" />
							</listcell>
							<listcell>
								<label
									value="@load(each.teamVisitor.name)" />
							</listcell>
							<listcell>
								<label value="@load(each.goalsLocal)" />
							</listcell>
							<listcell>
								<label value="@load(each.goalsVisitor)" />
							</listcell>
							<listcell>
								<label value="@load(each.dateMatch)" />
							</listcell>
							<listcell>
								<label value="@load(each.teamLocal.stadium.name)" />
							</listcell>
							<listcell>
								<label value="@load(each.teamLocal.stadium.capacity)" />
							</listcell>							
							<listcell>
								<label value="@load(each.spectators)" />
							</listcell>
							<listcell>
								<button label="delete"
									onClick="@command('delete',match=each)">
								</button>
							</listcell>
						</listitem>
					</template>
				</listbox>

			</groupbox>
			<separator height="10px" />
		</vbox>
			
		<vbox>
		</vbox>
	</window>

</zk>